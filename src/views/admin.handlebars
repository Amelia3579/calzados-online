<h1> Registered Users</h1>

<div class="backgroundContainer adminContainer" >
  {{#each users}}

    <ul class="ul">
      <li class="li">
        <p class="pStyles">{{this.first_name}} {{this.last_name}}</p>
        <p class="pStyles">{{this.role}}</p>
        <p class="pStyles">{{this.last_connection}}</p>
        <button onclick="removeUser(`{{this._id}}`)" class="btnUser">Remove User</button>
      </li>
    </ul>

  {{/each}}
</div>

<script src="/js/user.js"></script>